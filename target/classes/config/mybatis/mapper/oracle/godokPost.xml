<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sql.godokPost">

	<select id="selectAll" parameterType="map" resultType="PostVO">
		select * 
		 from GodokPost
	</select>
	
	<resultMap type="PostVO" id="selResultMap">
		<id column="no" jdbcType="BIGINT" property="no"/>
		<result column="threadNo" jdbcType="BIGINT" property="threadNo"/>
		<result column="pwd" jdbcType="VARCHAR" property="pwd"/>
		<result column="regdate" jdbcType="TIMESTAMP" property="regdate"/>	
	</resultMap>
	
	<insert>
	insert into godokPost(no,threadNo,pwd,regdate)
	values(#{no},#{threadNo},#{pwd},#{regdate})
	</insert>
	
</mapper>









